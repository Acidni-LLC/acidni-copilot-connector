# Terprint AI Copilot Connector - Production Environment Template
# ================================================================
# Copy this file to .env.prod and fill in the values.
# DO NOT commit .env.prod to source control.
# ================================================================

TEAMSFX_ENV=prod

# Azure API Management - Production
APIM_BASE_URL=https://apim-terprint-prod.azure-api.net
APIM_DOMAIN=apim-terprint-prod.azure-api.net
APIM_SUBSCRIPTION_KEY_REFERENCE=<your-apim-subscription-key-name>

# App Configuration
APP_NAME_SUFFIX=

# Teams App Registration
TEAMS_APP_ID=<your-teams-app-id>
TEAMS_APP_TENANT_ID=<your-tenant-id>

# AAD App Registration
AAD_APP_CLIENT_ID=<your-aad-app-client-id>
AAD_APP_OBJECT_ID=<your-aad-app-object-id>
AAD_APP_TENANT_ID=<your-tenant-id>
AAD_APP_OAUTH_AUTHORITY=https://login.microsoftonline.com/<your-tenant-id>
AAD_APP_OAUTH_AUTHORITY_HOST=https://login.microsoftonline.com

# Bot Registration
BOT_ID=<your-bot-id>
BOT_ENDPOINT=https://terprint-bot-prod.azurewebsites.net

# OAuth Connection
OAUTH_CONNECTION_ID=<your-oauth-connection-id>

# Microsoft Partner Network ID (required for marketplace)
MPN_ID=<your-mpn-id>

# Portal URLs
PORTAL_BASE_URL=https://portal.terprint.net

{
  "$schema": "https://developer.microsoft.com/json-schemas/copilot/plugin/v2.2/schema.json",
  "schema_version": "v2.2",
  "name_for_human": "Terprint AI",
  "description_for_human": "Cannabis strain intelligence - strains, lab data, terpenes, dispensaries",
  "namespace": "terprint",
  "description_for_model": "Terprint AI v3.8.0 - Cannabis strain intelligence with lab data.\n\nAvailable tools:\n- getVersion: API health and version info\n- getStrains: List/search cannabis strains\n- getBatches: Get lab test data with terpene and cannabinoid profiles\n- searchData: Full-text search for strains and batches\n- getLocations: Find dispensary locations\n\nUse getBatches for COA/lab data questions. Use searchData for general queries.",
  "functions": [
    {
      "name": "getVersion",
      "description": "Get API health and version info. Use when asked about version or identity.",
      "parameters": {
        "type": "object",
        "properties": {}
      },
      "capabilities": {
        "response_semantics": {
          "data_path": "$"
        }
      }
    },
    {
      "name": "getStrains",
      "description": "List or search cannabis strains. Returns strain names, types, and details.",
      "parameters": {
        "type": "object",
        "properties": {
          "strain_name": {
            "type": "string",
            "description": "Filter by strain name (partial match)"
          },
          "type": {
            "type": "string",
            "description": "Filter by type: indica, sativa, hybrid"
          },
          "limit": {
            "type": "integer",
            "description": "Max results (default 25)"
          }
        }
      },
      "capabilities": {
        "response_semantics": {
          "data_path": "$.data"
        }
      }
    },
    {
      "name": "getBatches",
      "description": "Get lab test batches with terpene and cannabinoid profiles. Use for COA data, THC%, terpene profiles.",
      "parameters": {
        "type": "object",
        "properties": {
          "strain_name": {
            "type": "string",
            "description": "Filter by strain name"
          },
          "dispensary_id": {
            "type": "string",
            "description": "Filter by dispensary"
          },
          "min_thc": {
            "type": "number",
            "description": "Minimum THC percentage"
          },
          "min_terpenes": {
            "type": "number",
            "description": "Minimum total terpenes percentage"
          },
          "include": {
            "type": "string",
            "description": "Include profiles: terpenes, cannabinoids, or full"
          },
          "limit": {
            "type": "integer",
            "description": "Max results (default 25)"
          }
        }
      },
      "capabilities": {
        "response_semantics": {
          "data_path": "$.data"
        }
      }
    },
    {
      "name": "searchData",
      "description": "Full-text search for strains and batches. Use for general cannabis queries.",
      "parameters": {
        "type": "object",
        "required": ["q"],
        "properties": {
          "q": {
            "type": "string",
            "description": "Search query (required)"
          },
          "type": {
            "type": "string",
            "description": "Search type: strain, batch, or both"
          },
          "limit": {
            "type": "integer",
            "description": "Max results (default 20)"
          }
        }
      },
      "capabilities": {
        "response_semantics": {
          "data_path": "$.data"
        }
      }
    },
    {
      "name": "getLocations",
      "description": "List dispensary locations in Florida.",
      "parameters": {
        "type": "object",
        "properties": {
          "limit": {
            "type": "integer",
            "description": "Max results (default 50)"
          }
        }
      },
      "capabilities": {
        "response_semantics": {
          "data_path": "$.data"
        }
      }
    }
  ],
  "runtimes": [
    {
      "type": "OpenApi",
      "auth": {
        "type": "None"
      },
      "spec": {
        "url": "apiSpecificationFile/openapi.json"
      },
      "run_for_functions": ["getVersion", "getStrains", "getBatches", "searchData", "getLocations"]
    }
  ]
}

{
  "$schema": "https://developer.microsoft.com/json-schemas/teams/vDevPreview/MicrosoftTeams.schema.json",
  "manifestVersion": "devPreview",
  "version": "3.8.0",
  "id": "${{TEAMS_APP_ID}}",
  "developer": {
    "name": "Acidni LLC",
    "websiteUrl": "https://www.acidni.net",
    "privacyUrl": "https://www.acidni.net/privacy",
    "termsOfUseUrl": "https://www.acidni.net/terms"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Terprint AI",
    "full": "Terprint AI - Cannabis Intelligence"
  },
  "description": {
    "short": "AI-powered cannabis strain intelligence with lab data and personalized favorites",
    "full": "Terprint AI provides comprehensive cannabis intelligence including strain details, lab reports (COA), terpene profiles, dispensary locations, and deals in Florida. Version 3.8.0 introduces strong stock checking capabilities."
  },
  "accentColor": "#10B981",
  "copilotAgents": {
    "declarativeAgents": [
      {
        "id": "terprintAgent",
        "file": "declarativeAgent.json"
      }
    ]
  },
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "apim-terprint-dev.azure-api.net",
    "terprint.acidni.net"
  ]
}
{
  "name": "acidni-copilot-connector",
  "version": "1.0.0",
  "description": "Microsoft 365 Copilot plugin for Terprint AI cannabis analytics services",
  "main": "index.js",
  "scripts": {
    "validate": "npx @microsoft/teams-toolkit-cli validate -m ./appPackage/manifest.json",
    "package": "npx @microsoft/teams-toolkit-cli package -m ./appPackage/manifest.json -o ./appPackage/build",
    "test:api": "node scripts/test-api.js",
    "test": "npm run validate && npm run test:api",
    "build": "npm run package"
  },
  "keywords": [
    "microsoft-365",
    "copilot",
    "teams",
    "terprint",
    "cannabis",
    "ai"
  ],
  "author": "Acidni LLC",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/Acidni-LLC/acidni-copilot-connector.git"
  },
  "devDependencies": {
    "@microsoft/teams-toolkit-cli": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

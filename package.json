{
  "name": "acidni-copilot-connector",
  "version": "3.10.0",
  "description": "Microsoft 365 Copilot plugin for Terprint AI cannabis analytics services",
  "main": "index.js",
  "scripts": {
    "validate": "npx @microsoft/teamsapp-cli validate --manifest-file ./appPackage/manifest.json",
    "validate:schemas": "node scripts/validate-schemas.js",
    "version:sync": "node scripts/sync-version.js",
    "package": "npx @microsoft/teamsapp-cli package --manifest-file ./appPackage/manifest.json --output-package-file ./appPackage/build/appPackage.zip",
    "test:api": "node scripts/test-api.js",
    "test": "npm run validate && npm run validate:schemas && npm run test:api",
    "build": "npm run package"
  },
  "keywords": [
    "microsoft-365",
    "copilot",
    "teams",
    "terprint",
    "cannabis",
    "ai"
  ],
  "author": "Acidni LLC",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/Acidni-LLC/acidni-copilot-connector.git"
  },
  "devDependencies": {
    "@microsoft/teamsapp-cli": "^3.1.0",
    "ajv": "^8.12.0",
    "ajv-draft-04": "^1.0.0",
    "ajv-formats": "^2.1.1",
    "js-yaml": "^4.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
